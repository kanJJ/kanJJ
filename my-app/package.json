    "scripts": {
        "start": "webpack-dev-server --progress --inline --host localhost --port 3000 --config webpack/webpack.config.js",
        "dev": "webpack-dev-server --progress --inline --hot --host localhost --port 3000 --config webpack/webpack.config.dev.js",
        "uat-staff": "cross-env ENV_CONFIG=uat_staff npm run dev",
        "clean": "rimraf dist/*",
        "build-dev": "npm run clean && webpack -dp --display-error-details --config webpack/webpack.config.dev.js",
        "build": "npm run clean && webpack -p --profile --progress --colors --display-error-details --config webpack/webpack.config.js",
        "build-sit": "cross-env ENV_CONFIG=dev npm run build",
        "build-uat-cust": "cross-env ENV_CONFIG=uat_cust npm run build",
        "build-pro-cust": "cross-env ENV_CONFIG=pro_cust npm run build",
        "build-uat-staff": "cross-env ENV_CONFIG=uat_staff npm run build",
        "build-pro-staff": "cross-env ENV_CONFIG=pro_staff npm run build",
        "test": "mocha --compilers js:babel-core/register ./test/**/*.spec.js",
        "test-watch": "npm test -- --watch",
        "lint": "eslint --ext .js,.jsx src/ test/",
        "lint-html": "rimraf ./reports && npm run lint -- -f html -o ./reports/ESLint-Report.html",
        "lint-fix": "npm run lint -- --fix && npm run lint-html",
        "cover": "istanbul cover **/*.js"
    }
}
